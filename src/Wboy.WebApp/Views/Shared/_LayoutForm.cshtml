<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp">
    <title>@ViewBag.Title</title>
    <!--[if lt IE 9]>
    <meta http-equiv="refresh" content="0;ie.html"/><![endif]-->
    <link rel="shortcut icon" href="favicon.ico">
    <link href="~/css/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet">
    <link href="~/css/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet">
    <link href="~/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="~/css/plugins/select2/select2.css" rel="stylesheet" />
    <link href="~/css/select2.css" rel="stylesheet" />
    <link href="~/css/style.min862f.css?v=4.1.0" rel="stylesheet">
    <link href="~/js/plugins/validator/validator.css" rel="stylesheet" />
    @RenderSection("styles", false)
    <style type="text/css">
        .ibox {
            margin-bottom: 0px !important;
        }

        .ibox-content {
            border-width: 0px !important;
        }

        .select2-container .select2-choice {
            border-radius: 0px !important;
        }

        .select2-container-multi .select2-choices {
            -webkit-border-radius: 0px;
            -moz-border-radius: 0px;
            -ms-border-radius: 0px;
            -o-border-radius: 0px;
            border-radius: 0px !important;
        }

        .select2-drop {
            border-radius: 0px !important;
        }

        .select2-drop-active {
            border: 1px solid #20B293 !important;
            border-top: 0 !important;
        }

        .select2-container-multi.select2-container-active .select2-choices {
            border: 1px solid #20B293 !important;
        }

        .select2-container-active .select2-choice, .select2-container-active .select2-choices {
            border: 1px solid #20B293 !important;
        }

        .treeview {
            overflow-x: hidden !important;
        }

        label font {
            position: absolute;
            color: red;
            right: 0px;
        }

        .control-label {
            text-align: right;
            padding-top: 7px;
            margin-bottom: 0;
        }

        .col-sm-1 {
            width: 8.33333333% !important;
            float: left;
            padding-right: 15px;
            padding-left: 15px;
        }

        .col-sm-3 {
            width: 25% !important;
            float: left;
            padding-right: 15px;
            padding-left: 15px;
        }

        .col-sm-4 {
            width: 33.33333333% !important;
            float: left;
            padding-right: 15px;
            padding-left: 15px;
        }

        .col-sm-2 {
            width: 16.66666667% !important;
            float: left;
            padding-right: 15px;
            padding-left: 15px;
        }

        .col-sm-8 {
            width: 66.66666667% !important;
            float: left;
            padding-right: 15px;
            padding-left: 15px;
        }

        .col-sm-9 {
            width: 75% !important;
            float: left;
            padding-right: 15px;
            padding-left: 15px;
        }
        .ui-select-option {
            margin: 0px;
            padding: 0px;
            width: 100%;
            display: none;
            position: absolute;
            left: 0px;
            top: 34px;
            background-color: #fff;
            overflow: hidden;
            border: 1px solid #e5e6e7;
            border-top: 0;
            z-index: 9999;
            box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.1);
        }

        .ui-select-option-content {
            overflow-x: hidden !important;
            font-size: 9pt;
        }

        .select2-dropdown {
            font-size: 9pt;
        }

        .ui-select {
            cursor: pointer;
            outline: 0;
            width: auto;
            height: 34px;
            line-height: 34px;
            position: relative;
            background: #fff url(../../images/a2.png) no-repeat right center;
        }

            .ui-select .ui-select-text {
                -moz-user-select: none;
                -webkit-user-select: none;
                -ms-user-select: none;
                -khtml-user-select: none;
                user-select: none;
                border: 1px solid #e5e6e7;
                padding-left: 12px;
                font-size: 14px;
                height: 34px;
                overflow: hidden;
            }

                .ui-select .ui-select-text:focus {
                    border: 1px #25B092 solid;
                }

        .ui-select-option-search .form-control {
            box-shadow: none;
            border-color: #ccc;
            border: none;
            border-top: 1px solid #ccc;
            height: 25px;
            width: 100%;
            margin: 0px;
            font-size: 9pt;
        }

        .ui-select-option-search .input-query {
            position: absolute;
            right: 4px;
            color: #ccc;
            font-size: 16px;
            margin-top: 2px;
        }

        .treeview {
            overflow-x: hidden !important;
        }
    </style>
</head>
<body>
<div class="container">
    @RenderBody()
    @Html.AntiForgeryToken()
</div>
<script src="~/js/jquery.min.js?v=2.1.4"></script>
<script src="~/js/bootstrap.min.js?v=3.3.6"></script>
<script src="~/js/content.min.js?v=1.0.0"></script>
<script src="~/js/plugins/iCheck/icheck.min.js"></script>
<script src="~/css/plugins/select2/select2.min.js"></script>
<script src="~/css/plugins/select2/select2_locale_zh-CN.js"></script>
<script src="~/js/plugins/validator/validator.js"></script>
<script src="~/js/plugins/prettyfile/bootstrap-prettyfile.js"></script>
<script src="~/js/plugins/My97DatePicker/WdatePicker.js"></script>
<script src="~/js/common.js"></script>
@RenderSection("scripts", false)
</body>
</html>
